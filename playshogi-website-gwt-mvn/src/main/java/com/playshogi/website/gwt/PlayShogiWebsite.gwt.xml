<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
        "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to='playshogiwebsite'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name='com.google.web.bindery.event.EventBinder'/>
    <inherits name="com.google.gwt.inject.Inject"/>

    <inherits name="com.playshogi.playshogi-library-shogi"/>

    <!-- Style -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>
    <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.playshogi.website.gwt.client.PlayShogiMain'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>
    <resource path='resources'/>

    <!-- allow Super Dev Mode -->
    <add-linker name="xsiframe"/>
</module>
